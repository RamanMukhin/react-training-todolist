{
    "env": {
      "commonjs": true,
      "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["prettier"],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module"
    },
    "rules": {
      "prettier/prettier": [
        "warn",
        {
          "endOfLine": "auto"
        }
      ],
      "class-methods-use-this": "off",
      "no-underscore-dangle": "off"
    }
  }
